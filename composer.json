{
  "name": "paunin/laswagger",
  "description": "This package allows you to: generate RESTful API document using Swagger PHP",

  "require": {
    "php": ">=5.3.0",
    "illuminate/console": "5.*",
    "illuminate/routing": "5.*",
    "illuminate/support": "5.*",
    "zircote/swagger-php": "2.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "laravel/lumen-framework": "5.*"
  },

  "autoload": {
    "psr-4": {
      "Laswagger\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laswagger\\Test\\": "tests"
    }
  }
}
